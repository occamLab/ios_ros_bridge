<launch>

  <node pkg="ios_streamer" type="pose_server.py" name="pose_server">
   <param name="port_number" value="35601" type="int"/>
   <param name="pose_topic" value="ios_pose"/>
   <param name="coordinate_frame" value="odom"/>
  </node>

  <node pkg="ios_streamer" type="image_server.py" name="image_server_compressed" output="screen">
   <param name="port_number" value="35602" type="int"/>
   <param name="camera_name" value="camera_compressed"/>
 </node>

   <node pkg="ios_streamer" type="intrinsics_server.py" name="intrinsics_server" output="screen">
     <param name="port_number" value="35603" type="int"/>
     <param name="camera_name" value="camera_compressed"/>
  </node>

</launch>
